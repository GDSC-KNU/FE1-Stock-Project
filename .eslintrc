{
  "extends": ["plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react", "import", "react-hooks"],
  "rules": {     
    "prettier/prettier": ["error", { "endOfLine": "auto" }]   
  },
  "ignorePatterns": ["node_modules/"]
}